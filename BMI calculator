print("Welcome to the BMI Calculator.")

weight = int(input("What is your weight? "))
height = float(input("What is your height? "))

bmi = float(weight / (height**2))

print(f"Your BMI is {bmi}")

if bmi < 18.5:
    print("underweight")
elif bmi < 25:
    print("normal weight")
else:
    print("overweight")
